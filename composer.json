{
    "name": "billhance/remote-assets-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony2 console command that copies a remote file to the public assets directory",
    "keywords": ["assets", "command", "symfony"],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bill Hance",
            "email": "billhance@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Billhance\\RemoteAssetsBundle": ""
        }
    },
    "target-dir": "Billhance/RemoteAssetsBundle"
}
